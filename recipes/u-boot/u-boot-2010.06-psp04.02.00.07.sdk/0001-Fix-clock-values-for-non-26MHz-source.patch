-.word 10, 0, 0, 1
-.word 10, 0, 0, 1
-.word 10, 0, 0, 1
+.word 130,  2, 0, 1
+.word  20,  0, 0, 1
+.word 325, 11, 0, 1
 .word 10, 0, 0, 1
 
 core_36x_dpll_param:
-.word 200, 12, 0, 1
-.word 200, 12, 0, 1
-.word 200, 12, 0, 1
+.word 100,  2, 0, 1
+.word 400, 12, 0, 1
+.word 375, 17, 0, 1
 .word 200, 12, 0, 1
 
 per_36x_dpll_param:
 /*    SYSCLK    M       N      M2      M3      M4     M5      M6      m2DIV */
-.word 26000,    432,   12,     9,      16,     9,     4,      3,      1
-.word 26000,    432,   12,     9,      16,     9,     4,      3,      1
-.word 26000,    432,   12,     9,      16,     9,     4,      3,      1
+.word 12000,    360,    4,     9,      16,     9,     4,      3,      1
+.word 13000,    864,   12,     9,      16,     9,     4,      3,      1
+.word 19200,    360,    7,     9,      16,     9,     4,      3,      1
 .word 26000,    432,   12,     9,      16,     9,     4,      3,      1
 
 .globl get_36x_mpu_dpll_param
