+++ b/arch/arm/cpu/arm_cortexa8/omap3/lowlevel_init.S
@@ -363,32 +363,30 @@ get_per_dpll_param:
 
 /*
  * Tables for 36x/37x devices
- *
- * (Populated for 13MHz only)
  */
 mpu_36x_dpll_param:
-.word 300, 12, 0, 1
-.word 300, 12, 0, 1
-.word 300, 12, 0, 1
+.word 300,  5, 0, 1
+.word 600, 12, 0, 1
+.word 125,  3, 0, 1
 .word 300, 12, 0, 1
 
