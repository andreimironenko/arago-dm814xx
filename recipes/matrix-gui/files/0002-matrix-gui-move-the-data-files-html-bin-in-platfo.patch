index f132d86..3939972 100755
--- a/Makefile.build
+++ b/Makefile.build
@@ -1,7 +1,8 @@
 -include ../../Rules.make
 
 ENV_SETUP ?= ../../linux-devkit/environment-setup
-DESTDIR ?=
+DESTDIR ?= 
+PLATFORM ?= "dm3730-am3715-evm"
 
 SOURCES = main.cpp mainwindow.cpp mainwindow.h matrix_gui.pro
 
@@ -26,8 +27,8 @@ clean : ${SOURCES} qmake
 install: all
 	@install -d ${DESTDIR}/usr/bin
 	@install matrix_gui ${DESTDIR}/usr/bin
-	@install bin/* ${DESTDIR}/usr/bin
+	cp ${PLATFORM}/bin/* ${DESTDIR}/usr/bin/
 	@install -d ${DESTDIR}/usr/share/matrix/html
-	@install *.html ${DESTDIR}/usr/share/matrix/html
+	cp -ar ${PLATFORM}/html ${DESTDIR}/usr/share/matrix
 	@install -d ${DESTDIR}/usr/share/matrix/images
 	@install images/* ${DESTDIR}/usr/share/matrix/images
