---
 trunk/ti814x/etc/init |   10 +++++-----
 trunk/ti816x/etc/init |   10 +++++-----
 2 files changed, 10 insertions(+), 10 deletions(-)

diff --git a/trunk/ti814x/etc/init b/trunk/ti814x/etc/init
index f2e42f6..b37359b 100644
--- a/trunk/ti814x/etc/init
+++ b/trunk/ti814x/etc/init
@@ -12,11 +12,11 @@ export QWS_MOUSE_PROTO=Auto:/dev/input/mice
 
 case "$1" in
   start)
-    if [ ! -f /etc/pointercal ] ; then
-        echo -n "Calibrating touchscreen (first time only)"
-        ts_calibrate
-        echo "."
-    fi
+#    if [ ! -f /etc/pointercal ] ; then
+#        echo -n "Calibrating touchscreen (first time only)"
+#        ts_calibrate
+#        echo "."
+#    fi
 
 	if [ -e $PIDFILE ]; then
       PIDDIR=/proc/$(cat $PIDFILE)
