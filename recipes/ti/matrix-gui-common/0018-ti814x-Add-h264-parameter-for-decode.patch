From b0764d0c6dcd8415491ad415f8229dce5b16e046 Mon Sep 17 00:00:00 2001
From: Siddharth Heroor <heroor@ti.com>
Date: Thu, 20 Oct 2011 21:04:15 +0530
Subject: [PATCH 18/19] ti814x: Add h264 parameter for decode


Signed-off-by: Siddharth Heroor <heroor@ti.com>
---
 trunk/ti814x/bin/runDecodeDisplayHDMI |    2 +-
 trunk/ti814x/bin/runDecodeDisplayLCD  |    2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/trunk/ti814x/bin/runDecodeDisplayHDMI b/trunk/ti814x/bin/runDecodeDisplayHDMI
index 86e2407..239a980 100755
--- a/trunk/ti814x/bin/runDecodeDisplayHDMI
+++ b/trunk/ti814x/bin/runDecodeDisplayHDMI
@@ -42,7 +42,7 @@ execute_demo()
 {
     cd /usr/share/ti/ti-omx
 
-    ./decode_display_a8host_debug.xv5T -w 1920 -h 1080 -f 30 -i /usr/share/ti/data/videos/dm816x_1080p_demo.264 -g 0 -d 0
+    ./decode_display_a8host_debug.xv5T -w 1920 -h 1080 -f 30 -i /usr/share/ti/data/videos/dm816x_1080p_demo.264 -g 0 -d 0 -c h264 
 }
 
 
diff --git a/trunk/ti814x/bin/runDecodeDisplayLCD b/trunk/ti814x/bin/runDecodeDisplayLCD
index d7cae27..4765ec4 100755
--- a/trunk/ti814x/bin/runDecodeDisplayLCD
+++ b/trunk/ti814x/bin/runDecodeDisplayLCD
@@ -42,7 +42,7 @@ execute_demo()
 {
     cd /usr/share/ti/ti-omx
 
-    ./decode_display_a8host_debug.xv5T -w 1920 -h 1080 -f 30 -i /usr/share/ti/data/videos/dm816x_1080p_demo.264 -g 0 -d 1
+    ./decode_display_a8host_debug.xv5T -w 1920 -h 1080 -f 30 -i /usr/share/ti/data/videos/dm816x_1080p_demo.264 -g 0 -d 1 -c h264 
 }
 
 
