From a2f918bfd04d1b67a39698c424b672d14254d25f Mon Sep 17 00:00:00 2001
From: Don Darling <don.osc2@gmail.com>
Date: Thu, 11 Mar 2010 17:40:52 -0600
Subject: [PATCH 5/9] Add support for NV16 colorspace.

---
 sys/v4l2/gstv4l2object.c |   11 +++++++++++
 1 files changed, 11 insertions(+), 0 deletions(-)

diff --git a/sys/v4l2/gstv4l2object.c b/sys/v4l2/gstv4l2object.c
index 290b767..3d1f254 100644
--- a/sys/v4l2/gstv4l2object.c
+++ b/sys/v4l2/gstv4l2object.c
@@ -676,6 +676,9 @@ static const guint32 gst_v4l2_formats[] = {
 #ifdef V4L2_PIX_FMT_YVYU
   V4L2_PIX_FMT_YVYU,
 #endif
+#ifdef V4L2_PIX_FMT_NV16
+  V4L2_PIX_FMT_NV16,
+#endif
 #ifdef V4L2_PIX_FMT_YUV422UVP
    V4L2_PIX_FMT_YUV422UVP
 #endif
@@ -1010,6 +1013,9 @@ gst_v4l2_object_v4l2fourcc_to_structure (guint32 fourcc)
 #ifdef V4L2_PIX_FMT_YVYU
     case V4L2_PIX_FMT_YVYU:
 #endif
+#ifdef V4L2_PIX_FMT_NV16
+    case V4L2_PIX_FMT_NV16:
+#endif
 #ifdef V4L2_PIX_FMT_YUV422UVP
     case V4L2_PIX_FMT_YUV422UVP:
 #endif
@@ -1055,6 +1061,11 @@ gst_v4l2_object_v4l2fourcc_to_structure (guint32 fourcc)
           fcc = GST_MAKE_FOURCC ('Y', 'V', 'Y', 'U');
           break;
 #endif
+#ifdef V4L2_PIX_FMT_NV16
+       case V4L2_PIX_FMT_NV16:
+         fcc = GST_MAKE_FOURCC ('N', 'V', '1', '6');
+         break;
+#endif
 #ifdef V4L2_PIX_FMT_YUV422UVP
        case V4L2_PIX_FMT_YUV422UVP:
          fcc = GST_MAKE_FOURCC ('Y', '8', 'C', '8');
